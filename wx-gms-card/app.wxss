/**app.wxss**/

@import '/common/reset.wxss';

@import '/common/layout.wxss';

.banner-swiper {
  height: 422rpx;
}
.prevent {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 999;
}
.gms-bg {
  z-index: -1;
  overflow: hidden;
}
.gms-bg.offset {
  overflow: hidden;
}
.gms-bg.offset > image {
  top: 100rpx;
}