/**app.wxss**/

page {
  font-size: 36rpx;
}
.hide {
  display: none !important;
}

.bg-white {
  background: #fff;
}

.pos-center {
  position: absolute;
  top: 50%; left: 50%;
  transform: translate(-50%,-50%);
}

.text {
  word-wrap: normal;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.text2 {
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-word;
  display: box;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.modal, .bg, .cover {
  position: absolute;
  top: 0; left: 0;
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.modal {
  opacity: 0;
  z-index: -1;
  overflow: hidden;
  transition: opacity .3s, z-index 0s .3s;
}
.modal-bg {
  background: rgba(0,0,0,.7);
}
.modal.in {
  opacity: 1;
  z-index: 1;
  transition: opacity .3s;
}
.modal-box {
  transform: translate3d(0,0,0);
}

.alert {
  position: absolute;
  top: 50%; left: 50%;
  transform: translate(-50%,-50%);
  width: 80%;
  max-width: 1000rpx;
  background: #fff;
  overflow: hidden;
  border-radius: 14rpx;
}
.alert {
  transform: translate3d(-50%,100%,0);
  transition: transform .3s cubic-bezier(.54,1,.65,1);
}
.in .alert {
  transform: translate3d(-50%,-50%,0);
}
.alert-head {
  padding: 40rpx 20rpx;
  text-align: center;
  font-size: 33rpx;
  color: #999;
  border-bottom: 1rpx solid #e5e5e5;
}
.alert-foot {
  display: flex;
  align-items: center;
  text-align: center;
  font-size: 34rpx;
  border-top: 1rpx solid #e5e5e5;
}
.alert-foot > .btn {
  height: 100rpx;
  line-height: 100rpx;
  flex: 1 0 auto;
  font-size: 1.1em;
  border-left: 1px solid #e5e5e5;
}
.alert-foot > .btn:first-child {
  border-left: 0;
}
.btn-confirm {
  color: #108EE9;
}


.image, .ratio {
  position: relative;
}
.image::before, .ratio::before {
  content: "";
  padding-top: 100%;
  display: block;
}
.image > image, .ratio > image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: block;
}

.bg > image, .cover > image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: block;
}

.btn {
  font: inherit;
  line-height: 1;
  background: none;
  flex-shrink: 0;
  border-radius: 0;
  display: flex;
  align-items: center;
  justify-content: center;
}
.btn::after {
  display: none;
}
.btn.button-hover {
  opacity: 0.85;
}


.form-group {
  margin-bottom: 15rpx;
}
.form-group input {
  flex-grow: 1;
}
.form-inline {
  display: flex;
  align-items: center;
}

.gap-t > view:not(:first-child),
.gap-t > button:not(:first-child) { margin-top: 20rpx; }
.gap-b > view:not(:last-child),
.gap-b > button:not(:last-child) { margin-bottom: 20rpx; }
.gap-l > view:not(:first-child),
.gap-l > button:not(:first-child) { margin-left: 20rpx; }
.gap-r > view:not(:last-child),
.gap-r > button:not(:last-child) { margin-right: 20rpx; }

.flex-row { display: flex; align-items: center; }
.flex-row > .grow { flex-grow: 1; overflow: hidden; }
.flex-row > view:not(.grow),
.flex-row > text:not(.grow) { flex-shrink: 0; }

.xb-buttons {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  font-size: 30rpx;
  line-height: 1;
}
.xb-buttons > button {
  margin: 0;
}
.xb-buttons > button:not(:last-child) {
  margin-right: 20rpx;
  color: #548EFF;
  padding: 0 30rpx;
}
.xb-buttons > button:last-child {
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 60rpx 0 80rpx;
  color: #fff;
  background: #548EFF;
  border-top-left-radius: 1000em;
  border-bottom-left-radius: 1000em;
  box-shadow: 0 2px 6px 1px rgba(100,158,246,0.57);
}

button.pure {
  background: none;
}
button.pure::after {
  display: none;
}
.pure.button-hover {
  background: none;
}



.login-list .list-item {
  font-size: 38rpx;
  padding: 1em 30rpx;
  text-align: left;
  border-bottom: 1rpx solid #efefef;
  transition: .2s;
  display: flex;
  align-items: center;
}
.login-list .list-item.choose {
  color: #fff;
  background: #488FFA;
}
.login-list .list-image {
  width: 2em;
  margin-right: 1em;
  background-size: 100% 100%;
}
.login-list .list-image image {
  border-radius: 100%;
}
.login-list .worker .list-image {
  background-image: url(data:image/png;base64,)
}
.login-list .student .list-image {
  background-image: url(data:image/png;base64,);
}
.quote:before { content: '（'; } 
.quote:after { content: '）'; }