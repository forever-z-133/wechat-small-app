/* pages/preFundsChangeHistoryDetail/index.wxss */

page {
  font-size: 28rpx;
}
.section {
  padding: 30rpx 30rpx;
  border-bottom: 1rpx solid #ededed;
}
.section:last-of-type {
  border-bottom: none;
}

.flex {
  display: flex;
  align-items: center;
}
.state-header {
  justify-content: space-between;
  background: #488FFA;
  color: #fff;
}
.state-header .label {
  font-size: 1.1em;
}
.state-header .label::before {
  content: "";
  width: 1.2em;
  height: 1.2em;
  display: block;
  margin-right: 10rpx;
  background: #eee;
}

.user-info {
  flex-wrap: wrap;
}
.user-info .label {
  width: 4em;
  margin-right: 20rpx;
}


.list-title {
  font-size: 1.1em;
  font-weight: bold;
}
.list-item {
  padding: 20rpx 0;
  border-bottom: 1rpx solid #f0f0f0;
}
.list-item:last-child {
  border-bottom: none;
  margin-bottom: -10rpx;
}
.list-desc {
  display: flex;
  margin-top: 15rpx;
  justify-content: space-between;
}


.campus-info .list-title {
  margin-bottom: 10rpx;
}
.campus-info .cell {
  display: flex;
  padding: 10rpx 0;
  align-items: flex-start;
}
.campus-info .cell .label {
  width: 4.4em;
  color: #9f9f9f;
  text-align: right;
  margin-right: 1em;
  flex-shrink: 0;
}

.final-info {
  text-align: right;
}
.final-info .label {
  padding-right: 15rpx;
}
.table {
  display: table;
  margin: 0 0 0 auto;
}
.table .cell {
  display: table-row;
}
.table .cell > view {
  display: table-cell;
}

.pay-button {
  position: fixed;
  bottom: 0;
  right: 0;
  left: 0;
  background: #fff;
  box-shadow: 0 0 5rpx rgba(0,0,0,.2);
}
.needPay {
  height: 100rpx;
}
.btn-pay {
  width: 250rpx;
  height: 100rpx;
  background: #488FFA;
  color: #fff;
  margin: 0 0 0 auto;
}

.list {
  display: -webkit-box;
  display: flex;
  flex-wrap: wrap;
  box-sizing: border-box;
}
.list-item {
  position: relative;
  flex-shrink: 0;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
}
.list-image {
  position: relative;
  width: 100%;
  flex-shrink: 0;
}
.list-image:before {
  content: "";
  display: block;
  padding-top: 100%;
}
.list-image.normal::before {
  padding-top: 56.25%;
}
.list-image.slide::before {
  padding-top: 75%;
}
.list-image > img {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.list-content {
  flex-grow: 1;
  overflow: hidden;
  box-sizing: border-box;
}
.list-name,
.list-desc {
  min-height: 1em;
}
.list-sort,
.list-other {
  flex-shrink: 0;
}


.gap {
  height: 20rpx;
  background: #F2F3F4;
}
.state-header {
  padding-bottom: 90rpx;
}
.user-info {
  margin: -70rpx 30rpx 20rpx;
  background: #fff;
  border-radius: 10rpx;
  padding: 10rpx 25rpx;
  font-weight: bold;
  line-height: 60rpx;
  box-shadow: 0 15rpx 30rpx rgba(146, 161, 185, 0.2);
}
.user-info > .label {
  display: none;
}
.user-info .username {
  display: block;
}
.user-info .grand {
  margin-left: 40rpx;
}
.class-list .list-title::before,
.campus-info .list-title::before {
  content: "";
  position: relative;
  top: -.1em;
  width: 10rpx;
  height: 1em;
  display: inline-block;
  vertical-align: middle;
  margin-right: 20rpx;
  background: #488FFA;
  border-radius: 1000px;
  box-shadow: 0 4rpx 8rpx rgba(146, 161, 185, 0.6);
}
.class-list .list-box {
  padding: 10rpx 0 10rpx 30rpx;
}
.class-list .list-item {
  border: none;
  padding: 15rpx 0;
}
.class-list .list-desc {
  flex-wrap: wrap;
  font-size: .9em;
  color: #999999;
  margin-top: 8rpx;
}
.class-list .list-desc .price {
  flex-grow: 1;
  flex-basis: 100%;
}
.class-list .list-desc .cut,
.class-list .list-desc .total {
  flex-grow: 1;
}
/* .class-list .list-desc .price,
.class-list .list-desc .cut {
  flex-shrink: 0;
}
.class-list .list-desc .total {
  word-wrap: normal;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
} */
.campus-info .cell {
  padding: 5rpx 0 5rpx 20rpx;
}
.final-info .cell {
  padding: 5rpx 0;
}

.final-info .table {
  min-width: 80%;
  padding: 5rpx 0;
}
.final-info .cell > view {
  padding: 5rpx 0;
}
.final-info .label {
  color: #999999;
}
.final-info .label text {
  display: inline-block;
}
.final-info .countAll text:not(.label) {
  color: #000;
  display: inline-block;
  margin-right: 40rpx;
}
.state-header .label::before { background-position: 0 0; }
.state-header .label.paid::before { background-position: 0 50%; }
.user-info .username:before { background-position: 100% 0%; }
.user-info .mobile:before { background-position: 100% 50%; }
.user-info .grand:before { background-position: 100% 100%; }

.user-info .username:before,
.user-info .mobile:before {
  position: relative;
  top: -.1em;
}

.state-header .label::before,
.user-info .username:before,
.user-info .mobile:before,
.user-info .grand:before {
  content: "";
  width: 1.2em;
  height: 1.2em;
  display: inline-block;
  vertical-align: middle;
  margin-right: 10rpx;
  background-color: rgba(255,255,255,.0);
  background-size: 200% 300%;
  background-image: url(data:image/png;base64,);
}