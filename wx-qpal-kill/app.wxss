/**app.wxss**/
page {
  font-size: 30rpx;
}
.page > view {
  position: absolute;
  transform: translate3d(0,0,0);
}
image {
  width: 100%;
  height: 100%;
  display: block;
}
.page.hide {
  z-index: -2;
}
.ratio {
  position: relative
}
.ratio::before {
  content: "";
  display: block;
  padding-top: 100%;
}
.ratio > image,
.ratio > video {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.modal,
.bg, .full, .cover {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.bg > image,
.full > image,
.cover > image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.modal {
  opacity: 0;
  z-index: -1;
  transition: opacity .3s, z-index 0s .3s;
}
.modal-bg {
  background: rgba(0, 0, 0, .7);
}
.modal.in {
  opacity: 1;
  z-index: 5;
  transition: opacity .3s, z-index 0s;
}
.modal-box {
  transform: translate3d(0,0,0);
}

.drop {
  position: relative;
}
.drop::before {
  content: "";
  position: absolute;
  z-index: -1;
  top: -200rpx;
  left: 50%;
  width: 0;
  height: 200rpx;
  margin-left: -1px;
  border-left: 2px dotted rgba(255,255,255,.3);
}




@font-face {
	font-family: 'kdc';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}